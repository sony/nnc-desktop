{
  "name": "nnc-web",
  "version": "1.0.0",
  "private": true,
  "description": "",
  "main": "",
  "scripts": {
    "create-directory": "mkdirp ../dist/console/js && mkdirp ../dist/console/lib/css",
    "copy-error": "cp -r ./src/error ../dist/console/",
    "copy-html": "cp ./src/*.html ../dist/console/",
    "copy-image": "cp -r ./src/image ../dist/console",
    "copy-css": "cp -r ./src/css ../dist/console && cp ./node_modules/sweetalert2/dist/sweetalert2.min.css ../dist/console/lib/css/ && cp ./node_modules/vue2-timepicker/dist/VueTimepicker.css ../dist/console/lib/css/",
    "copy-browser": "cp -r ./src/browser/ ../dist/console",
    "copy-project-converter-js": "cp ../new_editor/shared_scripts/ProjectConverter.js ../dist/console/js/",
    "copy-empty-configuration-js": "cp ../new_editor/shared_scripts/EmptyConfiguration.js ../dist/console/js/",
    "copy-nnabla-corelib-defs-js": "cp ../new_editor/shared_scripts/nnabla-corelib-defs.js ../dist/console/js/",
    "copy-auth-js": "cp ../common/dist/auth.js ../dist/console/js/",
    "copy-misc-js": "npm run copy-project-converter-js && npm run copy-empty-configuration-js && npm run copy-nnabla-corelib-defs-js && npm run copy-auth-js",
    "copy-resource": "npm run copy-html && npm run copy-image && npm run copy-css && npm run copy-error && npm run copy-browser",
    "local-build": "npm run lint && npm run create-directory && cp ../common/env/local.js ../dist/console/js/environment.js && webpack -d && npm run copy-resource && npm run copy-misc-js",
    "local-build2": "npm run create-directory && webpack -d",
    "dev-build": "npm run lint && npm run create-directory && cp ../common/env/dev.js ../dist/console/js/environment.js && webpack -d && npm run copy-resource && npm run copy-misc-js",
    "qa-build": "npm run create-directory && cp ../common/env/qa.js ../dist/console/js/environment.js && webpack && npm run copy-resource && npm run copy-misc-js",
    "stage-build": "npm run create-directory && cp ../common/env/stg.js ../dist/console/js/environment.js && webpack && npm run copy-resource && npm run copy-misc-js",
    "prod-build": "npm run create-directory && cp ../common/env/prod.js ../dist/console/js/environment.js && webpack && npm run copy-resource && npm run copy-misc-js",
    "start": "npm run dev-build && sudo node ../local/server.js",
    "test": "karma start karma.config.js --single-run",
    "lint": "tslint 'src/**/*.ts{,x}' -e 'src/**/__tests__/**/*.ts{,x}'"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/chart.js": "^2.9.28",
    "@types/jasmine": "^2.6.0",
    "css-loader": "^1.0.1",
    "istanbul-instrumenter-loader": "^3.0.0",
    "jasmine": "^2.8.0",
    "karma": "^1.7.1",
    "karma-coverage-istanbul-reporter": "^1.3.0",
    "karma-jasmine": "^1.1.0",
    "karma-phantomjs-launcher": "^1.0.4",
    "karma-sourcemap-loader": "^0.3.7",
    "karma-typescript-preprocessor": "^0.3.1",
    "karma-webpack": "^2.0.5",
    "mini-css-extract-plugin": "^0.9.0",
    "mkdirp": "^0.5.1",
    "node-sass": "^4.14.1",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "phantomjs-polyfill-find": "0.0.1",
    "phantomjs-polyfill-find-index": "^1.0.1",
    "phantomjs-polyfill-includes": "^1.0.2",
    "phantomjs-polyfill-object-assign": "0.0.2",
    "phantomjs-prebuilt": "^2.1.15",
    "sass-loader": "^8.0.2",
    "style-loader": "^1.2.1",
    "ts-loader": "=5.4.5",
    "tslint": "^5.15.0",
    "tslint-loader": "^3.5.4",
    "typescript": "=2.9.2",
    "vue-loader": "=14.2.3",
    "vue-property-decorator": "=7.0.0",
    "webpack": "^4.39.3",
    "webpack-cli": "^3.3.7"
  },
  "dependencies": {
    "aws-sdk": "^2.713.0",
    "axios": "^0.18.1",
    "chart.js": "=2.9.4",
    "sweetalert2": "^6.10.2",
    "vue": "=2.5.17",
    "vue-class-component": "=6.2.0",
    "vue-property-decorator": "=7.0.0",
    "vue-router": "=3.0.1",
    "vue-template-compiler": "=2.5.17",
    "vue2-timepicker": "^1.1.1",
    "vuex": "=3.0.1"
  }
}
